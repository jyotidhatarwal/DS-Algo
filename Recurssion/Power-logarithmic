import java.io.*;
import java.util.*;

public class Main {

    public static void main(String[] args) throws Exception {
        // write your code here
        Scanner sc = new Scanner(System.in);
        int x = sc.nextInt();
        int n = sc.nextInt();
        int pow = power(x,n);
        System.out.println(pow);
        
    }

    public static int power(int x, int n){
        if(n == 0 ){
            return 1;
        }
        
        int xnb2 = power(x,n/2);
        int xn = xnb2*xnb2;
        
        if(n % 2 == 1){
            xn = xn*x;
        }
        
        return xn;
    }

}
